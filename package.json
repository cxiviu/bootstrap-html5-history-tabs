{
  "name": "bootstrap-history-tabs",
  "version": "2.0.0",
  "description": "Bootstrap tabs with that keeps track of the history using the HTML5 History API",
  "main": "index.html",
  "dependencies": {},
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "start": "parcel index.html --out-dir dist",
    "build": "parcel build index.html --out-dir build",
    "clean": "rm -rf .cache dist build",
    "format": "prettier --write . --single-quote"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffdavidgreen/bootstrap-history-tabs.git"
  },
  "keywords": [
    "jquery",
    "bootstrap",
    "tabs",
    "history",
    "html5"
  ],
  "author": "Jeff Green",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffdavidgreen/bootstrap-history-tabs/issues"
  },
  "homepage": "https://github.com/jeffdavidgreen/bootstrap-history-tabs"
}
